{
	"name": "spreadsheet-importer",
	"label": "Spreadsheet Importer",
	"description": "This gear takes a spreadsheet and appends it to the container metadata",
	"version": "0",
	"inputs": {
		"spreadsheet-file": {
			"base": "file",
			"description": "A spreadsheet to be appended to the metadata",
			"type": {
				"enum": [
					"tabular data"
				]
			}
		},
		"json-template": {
			"base": "file",
			"description": "A JSON template to validate DICOM data",
			"optional": false,
			"type": {
				"enum": [
					"source code"
				]
			}
		}
	},
	"config": {},
	"environment": {
		"GPG_KEY": "0D96DF4D4110E5C43FBFB17F2D347EA6AA65421D",
		"LANG": "C.UTF-8",
		"PATH": "/usr/local/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
		"PYTHON_PIP_VERSION": "19.0.3",
		"PYTHON_VERSION": "3.7.2"
	},
	"command": "python run.py",
	"author": "Kaleb Fischer",
	"maintainer": "Kaleb Fischer",
	"cite": "List citations here.",
	"license": "Other",
	"source": "",
	"url": "",
	"custom": {
		"gear-builder": {
			"category": "converter",
			"image": "flywheel/metadata-import-grp-1"
		}
	}
}
